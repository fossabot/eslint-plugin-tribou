{
  "name": "eslint-plugin-tribou",
  "version": "0.1.0",
  "description": "Start with JS Standard Style but push towards better readability, git-awareness, vim-usability, and React and ES6 best practices",
  "main": "index.js",
  "engines": {
    "node": ">=6.4.x <7",
    "npm": ">=4.5.x <5",
    "yarn": "0.24.6"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tribou/eslint-plugin-tribou.git"
  },
  "keywords": [
    "config",
    "eslint",
    "eslintconfig",
    "tribou",
    "flow",
    "flowtype",
    "javascript",
    "style",
    "styleguide",
    "styling",
    "lint",
    "linting",
    "readability"
  ],
  "author": "tribou",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tribou/eslint-plugin-tribou/issues"
  },
  "homepage": "https://github.com/tribou/eslint-plugin-tribou#readme",
  "dependencies": {
    "deepmerge": "^1.3.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-plugin-flowtype": "^2.34.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.10.3",
    "eslint-plugin-standard": "^2.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "nodemon": "^1.11.0"
  },
  "peerDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0"
  },
  "scripts": {
    "lint": "eslint --cache --ignore-path .gitignore index.js",
    "test": "eslint --config index.js test",
    "watch-test": "nodemon -x npm test",
    "yarn": "npm run --silent yarn-bin -- install --ignore-scripts",
    "yarn-bin": "node bin/yarn-${npm_package_engines_yarn}.js"
  }
}
